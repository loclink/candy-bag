{
  "compilerOptions": {
    "target": "ES5",
    "module": "ESNext",
    "outDir": "./dist",
    "declaration": true,
    "emitDeclarationOnly": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "skipLibCheck": true,
    "lib": ["ES2015", "ES2016", "ES2017", "ES5", "DOM", "ES2018", "ES2019", "ES2020", "ES2021", "ES2022"]
  }
}
